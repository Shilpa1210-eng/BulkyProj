@if (TempData["success"] != null)
{
    <script type="text/javascript">
        toastr.success('@TempData["success"]', 'Success', { timeOut: 5000, closeButton: true, progressBar: true });
    </script>
}

@if (TempData["error"] != null)
{
    <script type="text/javascript">
        toastr.error('@TempData["error"]', 'Error', { timeOut: 4000, closeButton: true, progressBar: true });
    </script>
}
